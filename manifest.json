{
	"name": "Wordle Helper",
	"version": "1.0",	
	"icons": {
      "16": "/icons/icon16.png",
	  "32": "/icons/icon32.png",
      "48": "/icons/icon48.png",
      "128": "/icons/icon128.png"
    },
	"description": "Helps solve the Wordle puzzle.",
	"manifest_version": 3,
	"action": {
		"default_popup": "popup.html",
		"default_icon": "/icons/icon32.png",
		"content_scripts": [{ "js": ["contentScript.js"] }]
	},
	"permissions": ["tabs", "<all_urls>"]
}